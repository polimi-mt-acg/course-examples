<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <!-- The Web Application display name, it can be displayed by some GUI tools -->
    <display-name>SOAPSoccerTeams</display-name>
    <servlet>
        <servlet-name>SOAPSoccerTeams</servlet-name>
        <servlet-class>com.github.polimi_mt_acg.SoccerTeamsImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SOAPSoccerTeams</servlet-name>
        <url-pattern>/*</url-pattern>
    </servlet-mapping>
</web-app>